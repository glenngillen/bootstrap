#!/bin/bash

brew install mas

# Install Things3
mas install 904280696
open /Applications/Things3.app/

# Install Be Focused Pro
mas install 961632517
open /Applications/Be\ Focused\ Pro.app/

# Install Fantastical
mas install 435003921
open /Applications/Fantastical.app/

# Install Noizio
mas install 928871589
open /Applications/Noizio.app/

# Install Dropshare
mas install 814499284
bin/sync_plist net.mkswap.Dropshare.plist
open /Applications/Dropshare.app/

# Install Divvy
mas install 413857545
bin/sync_plist com.mizage.Divvy.plist
open /Applications/Divvy.app/

brew cask install selfcontrol
bin/sync_plist org.eyebeam.SelfControl.plist
brew cask install rescuetime
open /Applications/RescueTime.app/
brew cask install keyboard-maestro
bin/sync_plist com.stairways.keyboardmaestro.plist
bin/sync_plist com.stairways.keyboardmaestro.engine.plist
bin/sync_plist com.stairways.keyboardmaestro.editor.plist
open /Applications/Keyboard\ Maestro.app/
